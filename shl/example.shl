Identifier : "(([a-zA-Z] | '_') ([a-zA-Z0-9] | '_')*)" {
  $0 = bold italic underlined black
  $1 = bold italic underlined black
}

Identifier : "(([a-zA-Z] | '_' | '$') ([a-zA-Z0-9] | '_')*)" {
  $0 = bold italic underlined black
  $1 = bold italic underlined black
}

